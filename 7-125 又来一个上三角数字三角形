#include<stdio.h>
int main()
{
    int n,t,cnt,m,s,a=1;
    scanf("%d",&n);
    for(m=n;m>0;m--)
    {
        t=n;cnt=1;
        printf("%4d",a);
        s=a;
        while(cnt<m)
        { s+=t;
            printf("%4d",s);
            t--;
            cnt++;
        }
        a++;
        printf("\n");
    }
}
