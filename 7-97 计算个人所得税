#include <stdio.h>
#include <math.h>
int main() 
{
    double sal,tax;
    scanf("%lf",&sal);
    if(sal<=1600)
    tax=0;
    else if(sal<=2500)
    tax=0.05*(sal-1600);
    else if(sal<=3500)
    tax=0.1*(sal-1600);
    else if(sal<=4500)
    tax=0.15*(sal-1600);
    else if(sal>4500)
    tax=0.2*(sal-1600);
    printf("%.2f",tax);
}
